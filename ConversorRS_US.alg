algoritmo "ConversorRS_US"
var
 reais, dolletas:real
inicio
      escreval("Quantos reais eu tenho? ")
      escreva("RS$: ")
      leia(reais)
      escreval(" ")
      dolletas <- reais/3
      escreval(" Posso ter US$:", dolletas:30:2)
       //escreval(" ") = espaçamento
        escreval(" ")
fimalgoritmo
